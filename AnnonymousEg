interface B{ //B.class
	void fn();
}
class D{
	int x;
	public void fn(){
		System.out.println("fn in D");
	}
	D(){x=10;}
	D(int x){this.x=x;System.out.println("D.X = " + x);}
	
}
abstract class C{ //C.class
	public abstract void fn();
	public abstract void fn2();
	public void fn3(){
		System.out.println("Fn3 in Abstract class C");
	}
}
public class AnonymousEg { //AnnonymousEg.class

	public static void main(String[] args) {
		//B b2=new B();
		B b=new B(){ //AnonymousEg$1.class implements B
			public void fn(){
				System.out.println("Inner class generated by JVM: AnonymousEg$1.class");
				System.out.println("fn from AnonymousEg$1.class");
			}
		};
		b.fn();
		C b2=new C(){ //AnonymousEg$2.class extends C
			public void fn(){
				System.out.println("Inner class generated by JVM: AnonymousEg$2.class");
				System.out.println("fn from AnonymousEg$2.class");
			}
			public void fn2(){
				System.out.println("Inner class generated by JVM: AnonymousEg$2.class");
				System.out.println("fn2 from AnonymousEg$2.class");
			}
		};
		b2.fn();
		b2.fn3();
		D d=new D(){ //AnonymousEg$3.class extends D
			public void fn(){
				System.out.println("Overridden fn in AnonymousEg$3.class extends D");
			}
		};
		D d2=new D(20){
			public void fn(){
				System.out.println("Overridden fn in AnonymousEg$4.class extends D");
			}
		};
		d.fn();
		d2.fn();
	}

}
